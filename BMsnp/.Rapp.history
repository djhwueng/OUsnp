    stack.pos<-c(1:ntip)#
    for(tipdownindex in k:1){#
      des.pos<-get(paste("I",Inode.length[1,tipdownindex],sep=""))#
     des.pos#
      if(length(intersect(stack.pos,des.pos))!=0){#
      insert.pos<-intersect(stack.pos,des.pos)#
      D[insert.pos,tipdownindex] <-1#
      }#
