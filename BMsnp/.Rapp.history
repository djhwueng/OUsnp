         }#
         kmtx<- rbind(insrow,kdown)#
         tree.D.anc.vcv<-kmtx%*%tree.D.anc.vcv%*%t(kmtx)#
         }else if(delrow.index==dim(kmtx)[1]){#only up#
         kup<- kdiag[1:(delrow.index-1),]#
         kmtx<- rbind(kup,insrow)#
         tree.D.anc.vcv<-kmtx%*%tree.D.anc.vcv%*%t(kmtx)#
